<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sumberbola">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconutama"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.sumberbola.SplashActivity"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen">

            <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>

        <activity android:name="com.sumberbola.MainActivity">
        </activity>

        <activity android:name="com.sumberbola.DetailBerita">
        </activity>

        <activity android:name="com.sumberbola.OpenPost">
        </activity>

        <activity android:name="com.sumberbola.AboutApp">
        </activity>

</application>

</manifest>